  sleep 1
  sleep 1
  play :eb5
  play :ab4
  play :f4
  sleep 1
  play :d5
  play :b4
  play :g4
  sleep 2
end


define :start_back do
  play :c4
  play :c3
  sleep 4
  play :b3
  play :bb2
  sleep 4
  play :a3
  play :ab2
  sleep 3
  play :g3
  play :g2
  sleep 1
  play :f3
  play :f2
  sleep 2
  play :d3
  play :d2
  sleep 2
end
live_loop :intro do
  4.times do
    start
  end
  stop
end
live_loop :intro_back do
  4.times do
    start_back
  end
  stop
end


