use_bpm 112
use_synth :piano
define :start do
  play :g5, sustain: 4
  play :c5, sustain: 4
  sleep 1
  sleep 0.5
  play :bb5, sustain: 4
  play :eb5, sustain: 4
  sleep 1
  sleep 0.5
  play :c6
  play :g5
  sleep 0.5
